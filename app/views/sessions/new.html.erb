<section class="home"> 
    <h1>SIGN IN</h1>
    <%= form_for(:session, url: signin_path) do |f| %>

        <%= f.label :email %>
        <%= f.email_field :email %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Sign in" %>
    <% end %>
</section>
