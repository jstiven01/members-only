<header>
  <div>
    <%= link_to "Member Only app", root_path, id: "logo" %>
  </div>
  <nav>
    <ul>
      <li><%= link_to "All Posts", posts_path %></li>
      <li><%= link_to "Create Post", new_post_path %></li>
        <% if logged_in? %>
          <ul>
            <li>
              <%= link_to "Sign out", signout_path, method: :delete %>
            </li>
          </ul>
      <% else %>
        <li><%= link_to "Sign in", signin_path %></li>
      <% end %>
    </ul>
  </nav>
</header>